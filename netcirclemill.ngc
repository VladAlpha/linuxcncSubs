(Circular Spiral Pocket)
#<tooldiam> = #1    (=3.000 tooldiameter)
#<od> = #2          (=30.000 Diameter)
#<xpos> = #4        (=15.000 centerX)
#<ypos> = #5        (=15.000 centerY)
#<zend> = #6        (=-5.000 Final Depth)
#<zsafe> = #7       (=2.000  safeZ)
#<sop> = #8         (=1.35 % stepover)
#<docp> = #9        (=-2.000 % DOC Z)
#<zfeed> = #10      (=100.00 feedrateZ)
#<feed> = #11       (=200.00 feedrateXY)
(calculate How many times into while)
#<runs>=FIX[[[#<od>-#<tooldiam>]/#<sop>]/2]
#<doc> = [#<docp>*#<tooldiam>/100]
#<sop> = [#<sop>*#<tooldiam>/100]

G0 X#<xpos> Y#<ypos>
G0 Z#<zsafe>
G1 Z0 F#<zfeed> (Move to Zero Z to start increment Z Cycle)
O95 while [#<zend> lt 0]
   O97 If [#<zend> lt #<doc>] 
    G1 G91 Z#<doc> 
   O97 else
        G1 G91 Z#<zend>
   O97 endif
#<zend>=[#<zend>-#<doc>]
#30=#<sop>
#31=1.0
#<od>1=1
G1 G90 X[#<xpos>+#30] F#<feed>
O10 while [#<od>1 lt #<runs>]
G3 X[#<xpos>-[#<od>1*#<sop>]] I-[[#31*#<sop>]]
#31=[#31+0.5]
#<od>1=[#<od>1+1]
G3 X[#<xpos>+[#<od>1*#<sop>]] I[[#31*#<sop>]]
#31=[#31+0.5]
O10 endwhile
G3 X[#<xpos>-[#<od>/2.0]+[#<tooldiam>/2.0]] Y#<ypos> I-[[[#<xpos>+[#<od>1*#<sop>]]-[#<xpos>-[#<od>/2.0]+[#<tooldiam>/2.0]]]/2.0]
G3 I[[#<od>/2.0]-[#<tooldiam>/2.0]]
G1 G90 X#<xpos> Y#<ypos>
O95 endwhile
G0 G90 Z#<zsafe>
M5 
M30
