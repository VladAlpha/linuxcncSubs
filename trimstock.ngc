(info:  trim subroutine)
o<trimSub> sub

#<tool> = #1    (=0 Tool | 0 == current)
#<rot> = #2     (=0 Spindle Rotation 3= CW 4 = CCW)
#<feedtype> = #3 (=0 NON-FUNC climb = 1 conv = 2)
#<rpm> = #4     (=0 RPM | 0 == current)
#<feed> = #5    (=0 feed | 0 == current)
#<sop> = #6     (=50 percent stepover)
#<doc> = #6     (=25 percent z step)
#<finish> = #7  (=10 percent finish stepover)
#<zsafe> = #8   (=0.1 pre/post travel height)
#<xpos> = #9    (=0 x start position)
#<ypos> = #10   (=0 y start position)
#<zstart> = #11 (=0 start height of feature)
#<zend> = #12   (=-1 bottom location)
#<startangle> = #13     (=90 angle of first side)
#<endangle> = #14       (=90 angle of second side)
#<xlength> = #15        (=5 length along x axis)
#<ylength> = #16        (=1 length along y axis)

(set or modify or retain machine settings)
    o30 if [#<tool> gt 0] (set tool)
        M6 T#<tool>
        o30 endif
    o31 if [#<rot> gt 0] (start spindle )
        M#<rot>
        o31 endif
    o33 if [#<rpm> gt 0] (set spindle speed)
        S#<rpm>
        o33 endif
    o34 if [#<feed> gt 0]
        F#<feed>
        o34 endif
(o2n error checking)
    o20 if [#5410 eq 0]
    (msg, YOU MUST LOAD A NON 0 TOOL: FAIL AT TEMPLATE)
    m2
    o20 endif
(usefull variable set)
    #<tooldiam> = #5410
    #<toolrad> = [#5410/2]
    #<step> = [#<tooldiam>*#<sop>/100]
    #<doc> = [#<tooldiam>*#<dop>/100]
    #<x1> = [#<xpos>-#<toolrad>-#<toolrad>/tan[#<startangle>]]
    #<y1> = [#<ypos>-#<toolrad>]
    #<x2> = [#<xpos>-#<toolrad>+[#<toolrad>+#<ylength>]/tan[#<startangle>]]
    #<y2> = [#<ypos>+#<ylength>+#<toolrad>]
    #<x3> = [#<xpos>+#<xlength>+#<toolrad>+[#<toolrad>+#<ylength>]/tan[#<endangle>]]
    #<y3> = [#<y2>]
    #<x4> = [#<xpos>+#<xlength>+#<toolrad>-#<toolrad>/tan[#<endangle>]]
    #<y4> = #<y1>

    #<count> = 0
    #<passes> = [fup[#<zstart>-#<zend>]/#<doc>]
    #<doc>= [[#<zstart>-#<zend>]/#<passes>]

G00 Z#<zsafe>
G00 X#<x1> Y#<y1>
G01 Z#<zstart>
o<firstside> while [#<count> lt #<passes>]
    G91 G01 Z-#<doc>
    G90 G01 X#<x2> Y#<y2>
    #<count> = [#<count>+1]
    o<firstsidenest> while [#<count> lt #<passes>]
        G91 G01 Z-#<doc>
        G90 G01 X#<x1> Y#<y1>
        #<count> = [#<count>+1]
    o<firstsidenest> endwhile
o<firstside> endwhile
G00 #<zsafe>
G00 #<x3> #<y3>
G00 #<zstart>

o<secondside> while [#<count> lt #<passes>]
    G91 G01 Z-#<doc>
    G90 G01 X#<x4> Y#<y4>
    #<count> = [#<count>+1]
    o<secondsidenest> while [#<count> lt #<passes>]
        G91 G01 Z-#<doc>
        G90 G01 X#<x3> Y#<y3>
        #<count> = [#<count>+1]
    o<secondsidenest> endwhile
o<secondside> endwhile
G00 #<zsafe>


o<trimSub> endsub